<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.smic.cf.mapper.ForeignStatisticTrendChartDataMapper">

    <select id="getMaxDateId" resultType="java.lang.String">
        select max(date_id)
        from adminui.sys_foreign_statistics_trend_chart_data
        where location_id = #{locationId}
    </select>
</mapper>