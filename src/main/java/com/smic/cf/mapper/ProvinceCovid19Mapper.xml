<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.smic.cf.mapper.ProvinceCovid19Mapper">

    <select id="getTotalSuspectedCount" resultType="java.lang.Integer">
        select sum(suspected_count)
        from adminui.sys_province_covid19;
    </select>
</mapper>